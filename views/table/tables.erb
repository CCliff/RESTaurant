<h1>Tables</h1>
<h2><a href="tables/new">Add a new table.</a></h2>

<ul>
	<% @tables.each do |table| %>
		<% if table.paid == false %>
		<li>
			<a href="/tables/<%=table.id%>">
				<%= table.number %>
			</a>            
			<a href="/tables/<%=table.id%>/edit">edit</a>
		</li>
		<% end %>
	<% end %>
</ul>