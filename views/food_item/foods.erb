<h1>Menu</h1>
<a class="button" href="foods/new"><div class="button">Add a food item to the menu.</div></a>

	<% @foods.each do |food| %>
	<div class="menu-item">
		<a href="/foods/<%=food.id%>">
			<div class="item">
				<%= food.name %>
			</div>
		</a>    

		<a href="/foods/<%=food.id%>/edit">
			<div class="edit">
				edit
			</div>
		</a>	
	</div>

	<% end %>